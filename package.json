{
  "name": "rag-bot",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "start": "node dist/index.mjs",
    "lint": "biome lint",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "format:check": "biome format",
    "test": "vitest run",
    "test:domain": "vitest run src/core/application/${TEST_DOMAIN}",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/zod-validator": "^0.7.5",
    "@line/bot-sdk": "^10.5.0",
    "@llamaindex/google": "^0.3.22",
    "@llamaindex/openai": "^0.4.21",
    "@llamaindex/postgres": "^0.0.65",
    "@llamaindex/workflow": "^1.1.24",
    "css-select": "^6.0.0",
    "domhandler": "^5.0.3",
    "domutils": "^3.2.2",
    "drizzle-orm": "^0.44.7",
    "gpt-tokenizer": "^3.4.0",
    "hono": "^4.10.6",
    "htmlparser2": "^10.0.0",
    "llamaindex": "^0.12.0",
    "node-jose": "^2.2.0",
    "openai": "^6.9.1",
    "p-limit": "^7.2.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "tsdown": "^0.16.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.12"
  },
  "pnpm": {
    "overrides": {
      "tinypool": "1.0.2"
    },
    "onlyBuiltDependencies": [
      "esbuild",
      "tree-sitter"
    ]
  }
}
